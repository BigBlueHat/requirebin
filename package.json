    "ecstatic": "0.3.2",
    "jsonp": "0.0.3",
    "filed": "0.0.7",
    "javascript-editor": "0.1.0",
    "cookie": "0.0.5",
    "uglify-js": "2.2.3",
    "browser-module-sandbox": "1.1.0",
    "github-oauth": "0.0.4",
    "concat-stream": "1.0.0",
    "hat": "0.0.3",
    "request": "2.12.0",
